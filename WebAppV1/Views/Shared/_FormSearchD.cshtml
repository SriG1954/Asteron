<!--Form Search-->
<div class="form-inline">
    <div class="form-group mx-sm-1">
        @Html.DropDownList("column", new SelectList(ViewBag.searchcolumns, "Value", "Text"), new { @class = "form-control btn-outline-primary dropdown-toggle" })
    </div>
    <div class="form-group mx-sm-1">
        <input type="text" class="form-inline" id="search" name="search" value="@ViewBag.search" placeholder="search">
    </div>
    <div class="form-group mx-sm-1">
        @Html.DropDownList("column1", new SelectList(ViewBag.searchcolumns1, "Value", "Text"), new { @class = "form-control btn-outline-primary dropdown-toggle" })
    </div>
    <div class="form-group mx-sm-1">
        <input type="text" class="form-inline" id="search1" name="search1" value="@ViewBag.search1" placeholder="search">
    </div>
    <button type="submit" id="btnsearch" value="Search" class="btn btn-primary mx-sm-1">Search</button>
</div>
