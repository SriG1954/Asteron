<!--Report Not Found Search-->
@using AppCoreV1.ABLEModels;
@using AppCoreV1.Repositories;
@using AppCoreV1.Helper;

<script src="~/js/ablereport.js"></script>

<div class="wrapper">
    <!-- Sidebar  -->
    <partial name="_SidebarAR" />
    <!-- Page Content  -->
    <div id="content">
        <div class="container-fluid">
            <div class="card">
                <div class="card-header">
                    <div class="card-title">
                        <div class="row">
                            <div style="padding-left:10px;">
                                <h4>@ViewData["Title"] report data not found</h4>
                                <a asp-action="Index" asp-controller="Able">Back to Home</a>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="card-body">
                    <div class="container">
                        <div class="jumbotron">
                            <h4 class="display-4">Hello, @User.Identity?.Name!</h4>
                            <p class="lead">Sorry for any inconveniance. <h2>@ViewData["Title"] report data not found</h2>.</p>
                            <hr class="my-4">
                            <p>Please try after some time</p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<partial name="_ModalPage" />
<partial name="_ModalPage" />

