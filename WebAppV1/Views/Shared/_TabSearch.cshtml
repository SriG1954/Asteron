<!--Form Search-->
<div class="form-inline">
    <div class="form-group mx-sm-3 mb-2">
        @Html.DropDownList("tabsearch", new SelectList(ViewBag.searchcolumns, "Value", "Text"), new { @class = "form-control btn-outline-primary dropdown-toggle", @id = "tabsearch" })
    </div>
    <div class="form-group mx-sm-3 mb-2">
        <input type="text" class="form-inline" id="txtsearch" name="search" value="@ViewBag.search" placeholder="search">
    </div>
    <button type="button" id="btnsearch" value="Search" class="btn btn-primary mb-2" onclick="return GetTabSearchResult()">Search</button>
    <div id="myspinner" style="padding-left:20px; visibility:hidden">
        <div class="spinner-border text-primary"
             id="spinner" role="status">
            <span class="sr-only">Loading...</span>
        </div>
    </div>
</div>
